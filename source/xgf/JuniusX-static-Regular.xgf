    <set nm="bot-base" p="80 57"/>
    <set nm="bot-b" p="96 87 73 64 50"/>
    <set nm="bot-a" p="100 83 77 60 54"/>
    <mv di="cap-height-overshoot" p="21">
      <sh p="109 113 41"/>
    </mv>
    <callm nm="cap-serif">
      base: 103,
      pt-a: 0,
      pt-b: 4,
      base-sh: bot-base,
      pt-a-sh: bot-a,
      pt-b-sh: bot-b
    </callm>
    <callm nm="mid-stem">
      ref1: 103, ref2: 21, pt1: 92, pt2: 106
    </callm>
    <sh r="92" p="69"/>
    <sh r="106" p="110"/>
  </gl>

  <gl pnm="uniA734">
    <!-- AO -->
    <set nm="bot-b" p="23 11"/>
    <set nm="bot-a" p="27 7"/>
    <mv di="cap-height-overshoot" p="54">
      <sh p="83"/>
    </mv>
    <callm nm="cap-serif">
      base: 30,
      base-sh: 4,
      pt-a: 33,
      pt-b: 37,
      pt-a-sh: bot-a,
      pt-b-sh: bot-b
    </callm>
    <callm nm="mid-stem">
      ref1: 54, ref2: 30, pt1: 19, pt2: 74
    </callm>
    <callm nm="cap-round-stem">
      pt-a: 0, pt-b: 84, stem-cv: cprs
    </callm>
    <callm nm="cap-round-stem">
      pt-a: 64, pt-b: 94, cv: cpho, stem-cv: cprs
    </callm>
  </gl>

  <gl pnm="uniA736">
