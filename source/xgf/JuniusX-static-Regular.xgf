  <gl pnm="AE">
    <set nm="bot-a" p="104 61 55"/>
    <set nm="bot-b" p="100 65 42"/>
    <set nm="top-b" p="69 36"/>
    <callm nm="cap-serif">
      <pmset>
	base: 107,
	pt-a: 0,
	pt-a-sh: 104,
	pt-b: 4,
	pt-b-sh: 100,
      </pmset>
      <pmset>
	base: 58,
	pt-a: 61,
	pt-a-sh: 55,
	pt-b: 65,
	pt-b-sh: 42,
	ppem: 250
      </pmset>
      <pmset>
	base: 20,
	pt-a: 17,
	pt-a-sh: 23,
	pt-b: 13,
	pt-b-sh: top-b,
