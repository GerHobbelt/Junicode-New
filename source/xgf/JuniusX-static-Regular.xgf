    <callm nm="mid-stem-serif">
      ref1: 107, ref2: 20, pt1: 94, pt2: 70, sh1: 89, sh2: 75
    </callm>
  </gl>

  <gl pnm="AE.alt1">
    <callm nm="cap-serif">
      <pmset>
	base: 82,
	pt-a: 0,
	pt-a-sh: 79,
	pt-b: 85,
	pt-b-sh: 66,
	cup-cv: cap-serif-undershoot,
	ppem: 250
      </pmset>
      <pmset>
	base: 19,
	pt-a: 16,
	pt-a-sh: 22,
	pt-b: 12,
	pt-b-sh: 37,
	base-cv: cap-height,
	cup-cv: cap-serif-overshoot,
	ppem: 250
      </pmset>
