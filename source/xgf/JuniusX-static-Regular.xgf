	pt-b-sh: bot-b
      </pmset>
      <pmset>
	base: 55,
	base-sh: 90,
	pt-a: 52,
	pt-a-sh: top-a,
	pt-b: 48,
	pt-b-sh: top-b,
	base-cv: cap-height,
	cup-cv: cap-serif-overshoot
      </pmset>
    </callm>
    <callm nm="mid-stem">
      ref1: 55, ref2: 30, pt1: 19, pt2: 43
    </callm>
    <callm nm="cap-round-stem">
      pt-a: 0, pt-b: 73, stem-cv: cprs
    </callm>
  </gl>

  <gl pnm="uniA738">
    <!-- AV -->
    <mv di="cap-height-overshoot" p="40">
