	<ip p="10 4"/>
	<mv col="w" p="34">
	  <mv col="g" p="59">
	    <ip p="45 38"/>
    </mv></mv></mv></mv>
  </gl>

  <gl pnm="uniA724">
    <!-- Egyptological Ain -->
