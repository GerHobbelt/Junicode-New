  <mo nm="cap-serif">
    <pm nm="base"/>
    <pm nm="base-sh" val="-1"/>
    <pm nm="pt-a"/>
    <pm nm="pt-a-sh" val="-1"/>
    <pm nm="pt-b"/>
    <pm nm="pt-b-sh" val="-1"/>
    <pm nm="base-cv" val="baseline"/>
    <pm nm="cup-cv" val="cap-serif-undershoot"/>
    <pm nm="ppem" val="160"/>
    <mv di="base-cv" p="base">
      <sh compile-if="not( nan(base-sh) ) and base-sh &gt;= 0" p="base-sh"/>
      <sh compile-if="nan(base-sh)">
        <set ref="base-sh"/>
      </sh>
      <mv di="cap-serif" p="pt-b">
        <sh compile-if="not( nan(pt-b-sh) ) and pt-b-sh &gt;= 0" p="pt-b-sh"/>
        <sh compile-if="nan(pt-b-sh)">
          <set ref="pt-b-sh"/>
        </sh>
      </mv>
    </mv>
    <callf nm="serif-helper">
      base: base,
      pt-a: pt-a,
      base-cv: base-cv,
      cup-cv: cup-cv,
      ppem: ppem
    </callf>
    <sh compile-if="not( nan(pt-a-sh) ) and pt-a-sh &gt;= 0" r="pt-a" p="pt-a-sh"/>
    <sh compile-if="nan(pt-a-sh)" r="pt-a">
      <set ref="pt-a-sh"/>
    </sh>
  </mo>

  <mo nm="lc-serif">
    <pm nm="base"/>
    <pm nm="base-sh" val="-1"/>
    <pm nm="pt-a"/>
    <pm nm="pt-a-sh" val="-1"/>
    <pm nm="pt-b"/>
    <pm nm="pt-b-sh" val="-1"/>
    <pm nm="base-cv" val="baseline"/>
    <pm nm="cup-cv" val="lc-serif-undershoot"/>
    <pm nm="ppem" val="180"/>
    <mv di="base-cv" p="base">
      <sh compile-if="not( nan(base-sh) ) and base-sh &gt;= 0" p="base-sh"/>
      <sh compile-if="nan(base-sh)">
        <set ref="base-sh"/>
      </sh>
      <mv di="lc-serif" p="pt-b">
        <sh compile-if="not( nan(pt-b-sh) ) and pt-b-sh &gt;= 0" p="pt-b-sh"/>
        <sh compile-if="nan(pt-b-sh)">
          <set ref="pt-b-sh"/>
        </sh>
      </mv>
    </mv>
    <callf nm="serif-helper">
      base: base,
      pt-a: pt-a,
      base-cv: base-cv,
      cup-cv: cup-cv,
      ppem: ppem
    </callf>
    <sh compile-if="not( nan(pt-a-sh) ) and pt-a-sh &gt;= 0" r="pt-a" p="pt-a-sh"/>
    <sh compile-if="nan(pt-a-sh)" r="pt-a">
      <set ref="pt-a-sh"/>
    </sh>
  </mo>

  <mo nm="sc-serif">
    <pm nm="base"/>
    <pm nm="base-sh" val="-1"/>
    <pm nm="pt-a"/>
    <pm nm="pt-a-sh" val="-1"/>
    <pm nm="pt-b"/>
    <pm nm="pt-b-sh" val="-1"/>
    <pm nm="base-cv" val="baseline"/>
    <pm nm="cup-cv" val="sc-serif-undershoot"/>
    <pm nm="ppem" val="180"/>
    <mv di="base-cv" p="base">
      <sh compile-if="not( nan(base-sh) ) and base-sh &gt;= 0" p="base-sh"/>
      <sh compile-if="nan(base-sh)">
        <set ref="base-sh"/>
      </sh>
      <mv di="sc-serif" p="pt-b">
        <sh compile-if="not( nan(pt-b-sh) ) and pt-b-sh &gt;= 0" p="pt-b-sh"/>
        <sh compile-if="nan(pt-b-sh)">
          <set ref="pt-b-sh"/>
        </sh>
      </mv>
    </mv>
    <callf nm="serif-helper">
      base: base,
      pt-a: pt-a,
      base-cv: base-cv,
      cup-cv: cup-cv,
      ppem: ppem
    </callf>
    <sh compile-if="not( nan(pt-a-sh) ) and pt-a-sh &gt;= 0" r="pt-a" p="pt-a-sh"/>
    <sh compile-if="nan(pt-a-sh)" r="pt-a">
      <set ref="pt-a-sh"/>
    </sh>
  </mo>

  <mo nm="hook">
    <pm nm="pt-a"/>
    <pm nm="cv" val="-1"/>
    <pm nm="pt-b"/>
    <pm nm="pt-c"/>
    <compile-if test="cv &gt;= 0">
      <push>pt-c pt-b pt-a cv</push>
      <command nm="MIAP" modifier="1"/>
      <command nm="MDRP">
        <modifier type="col" val="b"/>
      </command>
      <command nm="MDRP">
        <modifier type="col" val="b"/>
      </command>
      <else>
        <push>pt-c pt-b pt-a</push>
        <command nm="MDAP" modifier="1"/>
        <command nm="MDRP">
          <modifier type="col" val="b"/>
        </command>
        <command nm="MDRP">
          <modifier type="col" val="b"/>
        </command>
      </else>
    </compile-if>
  </mo>

  <mo nm="cap-round-stem">
    <pm nm="pt-a"/>
    <pm nm="pt-b"/>
    <pm nm="cv" val="cap-baseline-undershoot"/>
    <pm nm="stem-cv" val="-1"/>
    <compile-if test="stem-cv != -1">
      <mv di="cv" p="pt-a">
        <mv di="stem-cv" p="pt-b"/>
      </mv>
      <else>
        <mv di="cv" p="pt-a">
          <mv p="pt-b"/>
        </mv>
      </else>
    </compile-if>
  </mo>

  <mo nm="lc-round-stem">
    <pm nm="pt-a"/>
    <pm nm="pt-b"/>
    <pm nm="cv" val="lc-baseline-undershoot"/>
    <pm nm="stem-cv" val="-1"/>
    <compile-if test="stem-cv != -1">
      <mv di="cv" p="pt-a">
        <mv di="stem-cv" p="pt-b"/>
      </mv>
      <else>
        <mv di="cv" p="pt-a">
          <mv p="pt-b"/>
        </mv>
      </else>
    </compile-if>
  </mo>

  <mo nm="mid-stem">
    <pm nm="ref1"/>
    <pm nm="ref2"/>
    <pm nm="pt-a"/>
    <pm nm="pt-b"/>
    <pm nm="cv" val="-1"/>
    <push>pt-a pt-a ref2 ref1</push>
    <command nm="SRP1"/>
    <command nm="SRP2"/>
    <command nm="IP"/>
    <command nm="MDAP" modifier="1"/>
    <compile-if test="cv &gt;= 0">
      <push>pt-b cv</push>
      <command nm="MIRP" modifier="11100"/>
      <else>
        <push>pt-b</push>
        <command nm="MDRP" modifier="11100"/>
      </else>
    </compile-if>
  </mo>

  <mo nm="mid-stem-serif">
    <pm nm="ref1"/>
    <pm nm="ref2"/>
    <pm nm="pt-a"/>
    <pm nm="pt-b"/>
    <pm nm="sh-a"/>
    <pm nm="sh-b"/>
    <pm nm="cv" val="-1"/>
    <push>sh-a pt-a pt-a ref2 ref1</push>
    <command nm="SRP1"/>
    <command nm="SRP2"/>
    <command nm="IP"/>
    <command nm="MDAP" modifier="1"/>
    <command nm="SHP" modifier="1"/>
    <compile-if test="cv &gt;= 0">
      <push>sh-b pt-b cv</push>
      <command nm="MIRP" modifier="11101"/>
      <command nm="SHP" modifier="0"/>
      <else>
        <push>sh-b pt-b</push>
        <command nm="MDRP" modifier="11101"/>
        <command nm="SHP" modifier="0"/>
      </else>
    </compile-if>
  </mo>

  <mo nm="cap-insular-serif">
    <pm nm="top"/>
    <pm nm="serif-a"/>
    <pm nm="serif-b"/>
    <mv di="cpho" p="top">
      <mv min-distance="no" p="serif-a">
        <mv di="cap-serif" p="serif-b"/>
      </mv>
    </mv>
  </mo>

  <mo nm="lc-ascender-top">
    <pm nm="p1"/>
    <pm nm="p2"/>
    <pm nm="p3"/>
    <mv di="lc-ascender" p="p1">
      <mv p="p2">
        <mv di="lc-serif" p="p3"/>
      </mv>
    </mv>
  </mo>

  <!--

      Uppercase Latin

  -->

  <gl pnm="A" assume-y="no">
    <pm nm="iup" val="1"/>
    <set nm="bot-b" p="53 44 32"/>
    <set nm="bot-a" p="57 40 36"/>
    <setvs axis="y"/>
    <mv di="cap-height-overshoot" p="23">
      <mv p="64"/>
    </mv>
    <callm nm="cap-serif">
      base: 59,
      pt-a: 0,
      pt-b: 4,
      base-sh: 38,
      pt-a-sh: bot-a,
      pt-b-sh: bot-b
    </callm>
    <callm nm="mid-stem">
      ref1: 59, ref2: 23, pt-a: 49, pt-b: 61, cv: cap-bar
    </callm>
    <compile-if test="iup != 0">
      <iup axis="y"/>
    </compile-if>
  </gl>

  <gl pnm="A.alt1">
    <mv di="cpho" p="6">
      <sh p="14"/>
      <mv p="43"/>
    </mv>
    <mv di="cpbu" p="0">
      <sh p="34"/>
      <mv p="50">
        <sh p="24"/>
      </mv>
    </mv>
  </gl>

  <gl pnm="Aring">
    <set nm="bot-b" p="47 38 26"/>
    <set nm="bot-a" p="51 34 30"/>
    <mv p="16">
      <mv p="9">
        <sh p="22"/>
        <ip p="59 65"/>
      </mv>
    </mv>
    <callm nm="cap-serif">
      base: 53,
      pt-a: 0,
      pt-b: 4,
      base-sh: 32,
      pt-a-sh: bot-a,
      pt-b-sh: bot-b
    </callm>
    <callm nm="mid-stem">
      ref1: 53, ref2: 16, pt-a: 43, pt-b: 55, cv: cap-bar
    </callm>
  </gl>

  <gl pnm="uni023A" assume-y="no"> <!-- A with stroke -->
    <pm nm="iup" val="1"/>
    <set nm="bot-b" p="53 44 32"/>
    <set nm="bot-a" p="57 40 36"/>
    <setvs axis="y"/>
    <mv di="cap-height-overshoot" p="23">
      <mv p="64"/>
    </mv>
    <callm nm="cap-serif">
      base: 59,
      pt-a: 0,
      pt-b: 4,
      base-sh: 38,
      pt-a-sh: bot-a,
      pt-b-sh: bot-b
    </callm>
    <callm nm="mid-stem">
      ref1: 59, ref2: 23, pt-a: 49, pt-b: 61, cv: cap-bar
    </callm>
    <compile-if test="iup != 0">
      <iup axis="y"/>
    </compile-if>
  </gl>


  <gl pnm="uniA732">
    <!-- AA -->
    <set nm="bot-base" p="80 57"/>
    <set nm="bot-b" p="96 87 73 64 50"/>
    <set nm="bot-a" p="100 83 77 60 54"/>
    <mv di="cap-height-overshoot" p="22">
      <sh p="108 113 41"/>
    </mv>
    <callm nm="cap-serif">
      base: 103,
      pt-a: 0,
      pt-b: 4,
      base-sh: bot-base,
      pt-a-sh: bot-a,
      pt-b-sh: bot-b
    </callm>
    <callm nm="mid-stem">
      ref1: 103, ref2: 22, pt-a: 92, pt-b: 106, cv: cap-bar
    </callm>
    <sh r="92" p="69"/>
    <sh r="106" p="110"/>
  </gl>

  <gl pnm="uniA734">
    <!-- AO -->
    <set nm="bot-b" p="23 11"/>
    <set nm="bot-a" p="27 7"/>
    <mv di="cap-height-overshoot" p="55">
      <sh p="84"/>
    </mv>
    <callm nm="cap-serif">
      base: 30,
      base-sh: 4,
      pt-a: 33,
      pt-b: 37,
      pt-a-sh: bot-a,
      pt-b-sh: bot-b
    </callm>
    <callm nm="mid-stem">
      ref1: 55, ref2: 30, pt-a: 19, pt-b: 75, cv: cap-bar
    </callm>
    <callm nm="cap-round-stem">
      pt-a: 0, pt-b: 85, stem-cv: cprs
    </callm>
    <callm nm="cap-round-stem">
      pt-a: 65, pt-b: 95, cv: cpho, stem-cv: cprs
    </callm>
  </gl>

  <gl pnm="uniA736">
    <!-- AU -->
    <set nm="bot-b" p="20 11"/>
    <set nm="bot-a" p="24 7"/>
    <set nm="top-a" p="49 71 77"/>
    <set nm="top-b" p="53 67 81"/>
