  <gl pnm="uniA73C">
    <!-- AY -->
    <mv di="cap-height-overshoot" p="47">
      <sh p="80"/>
    </mv>
    <callm nm="cap-serif">
      <pmset>
        base: 21,
        pt-a: 23,
      	pt-b: 27,
      	pt-a-sh: 19,
      	pt-b-sh: 15
      </pmset>
      <pmset>
        base: 64,
        pt-a: 62,
      	pt-a-sh: 66,
      	pt-b: 58,
      	pt-b-sh: 70,
      	base-cv: cap-height,
      	cup-cv: cap-serif-overshoot
      </pmset>
    </callm>
    <callm nm="mid-stem">
      ref1: 21, ref2: 64, pt-a: 11, pt-b: 77, cv: cap-bar
    </callm>
    <mv p="0">
      <mv p="6"/>
    </mv>
