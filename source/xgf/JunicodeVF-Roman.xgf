        pt-a: 16,
      	pt-b: 20,
      	pt-a-sh: 12,
      	pt-b-sh: 8
      </pmset>
      <pmset>
        base: 57,
        pt-a: 55,
      	pt-a-sh: 59,
      	pt-b: 51,
      	pt-b-sh: 63,
      	base-cv: cap-height,
      	cup-cv: cap-serif-overshoot
      </pmset>
