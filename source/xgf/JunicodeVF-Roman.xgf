      <pmset>
        base: 27,
        pt-a: 29,
        pt-b: 33,
        base-sh: 3,
        pt-a-sh: bot-a,
        pt-b-sh: bot-b
      </pmset>
      <pmset>
        base: 46,
        base-sh: 74,
        pt-a: 44,
        pt-a-sh: top-a,
        pt-b: 40,
        pt-b-sh: top-b,
        base-cv: cap-height,
        cup-cv: cap-serif-overshoot
      </pmset>
    </callm>
    <callm nm="mid-stem">
      ref1: 46, ref2: 27, pt-a: 16, pt-b: 91, cv: cap-bar
    </callm>
    <callm nm="cap-round-stem">
      pt-a: 0, pt-b: 60, stem-cv: cprs
    </callm>
  </gl>

  <gl pnm="uniA738">
