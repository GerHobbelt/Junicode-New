  <gl pnm="uniA73A">
    <!-- AVhorizontalbar -->
    <mv di="cap-height-overshoot" p="39">
      <sh p="3"/>
    </mv>
    <callm nm="cap-serif">
      <pmset>
        base: 14,
        pt-a: 16,
      	pt-b: 20,
      	pt-a-sh: 12,
      	pt-b-sh: 8
      </pmset>
      <pmset>
        base: 57,
        pt-a: 55,
      	pt-a-sh: 59,
      	pt-b: 63,
      	pt-b-sh: 51,
      	base-cv: cap-height,
      	cup-cv: cap-serif-overshoot
      </pmset>
    </callm>
    <callm nm="mid-stem">
      ref1: 57, ref2: 14, pt-a: 70, pt-b: 71, cv:cap-bar
    </callm>
    <mv di="cpbu" p="0"/>
