  <gl pnm="uni1EB2"> <!-- A with breve and hook -->
    <callg pnm="A">
      iup: 0
    </callg>
    <mv r="23" p="65" col="w">
      <mv p="102" col="g">
        <ip p="74 83 92"/>
      </mv>
    </mv>
